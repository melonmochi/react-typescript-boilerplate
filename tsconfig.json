{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "importHelpers": true,
    "jsx": "react",
    "module": "esnext",
    "lib": ["esnext", "dom"],
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "./dist/",
    "paths": {
      "@/*": ["./src/*"],
      "typings": ["typings"]
    },
    "sourceMap": true,
    "target": "esnext"
  }
}
